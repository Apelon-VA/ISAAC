            </Tab>
          <Tab closable="false" text="Database">
               <content>
                  <ScrollPane fx:id="dbTabScollPane_" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab closable="false" text="Database Dependencies">
               <content>
                  <ScrollPane fx:id="dbDependenciesTabScollPane_" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" />
