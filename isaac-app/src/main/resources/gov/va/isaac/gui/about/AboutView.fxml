          <Tab closable="false" text="Application">
               <content>
                  <ScrollPane fx:id="appTabScollPane_" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0">
