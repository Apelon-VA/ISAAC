<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="summaryPane" minHeight="-Infinity" minWidth="450.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.va.isaac.gui.refexViews.refexCreation.wizardPages.SummaryController">
	<top>
		<VBox alignment="TOP_CENTER" BorderPane.alignment="TOP_CENTER">
			<children>
				<Label text="Refex Summary" textAlignment="CENTER" BorderPane.alignment="CENTER" VBox.vgrow="NEVER">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
					<padding>
						<Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
					</padding>
				</Label>
				<GridPane hgap="5.0" vgap="5.0">
					<columnConstraints>
						<ColumnConstraints fillWidth="false" hgrow="NEVER" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
						<ColumnConstraints hgrow="ALWAYS" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints fillHeight="false" vgrow="NEVER" />
						<RowConstraints fillHeight="false" vgrow="NEVER" />
						<RowConstraints fillHeight="false" vgrow="NEVER" />
						<RowConstraints fillHeight="false" vgrow="NEVER" />
					</rowConstraints>
					<children>
						<Label text="Refex Name">
							<font>
								<Font name="System Bold" size="12.0" />
							</font>
						</Label>
						<Label text="Parent Concept" GridPane.rowIndex="1">
							<font>
								<Font name="System Bold" size="12.0" />
							</font>
						</Label>
						<Label text="Refex Type" GridPane.rowIndex="2">
							<font>
								<Font name="System Bold" size="12.0" />
							</font>
						</Label>
						<Label text="Refex Description" GridPane.rowIndex="3" GridPane.valignment="TOP">
							<font>
								<Font name="System Bold" size="12.0" />
							</font>
						</Label>
						<TextArea fx:id="actualRefexDescription" editable="false" minHeight="50.0" prefHeight="50.0" wrapText="true"
							GridPane.columnIndex="1" GridPane.rowIndex="3" />
						<TextField fx:id="actualRefexName" editable="false" GridPane.columnIndex="1" />
						<TextField fx:id="actualParentConcept" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
						<TextField fx:id="actualRefexType" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
					</children>
					<VBox.margin>
						<Insets bottom="5.0" left="5.0" right="5.0" />
					</VBox.margin>
				</GridPane>
			</children>
		</VBox>
	</top>
	<bottom>
		<HBox alignment="CENTER" fillHeight="false" spacing="30.0" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
				<Button fx:id="startOverButton" mnemonicParsing="false" text="Start Over" />
				<Button fx:id="commitButton" mnemonicParsing="false" text="Commit" />
			</children>
			<BorderPane.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</BorderPane.margin>
		</HBox>
	</bottom>
	<center>
		<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="50.0" prefHeight="50.0" BorderPane.alignment="CENTER"
			VBox.vgrow="ALWAYS">
			<BorderPane.margin>
				<Insets left="2.0" right="2.0" />
			</BorderPane.margin>
			<VBox.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</VBox.margin>
			<content>
				<GridPane fx:id="columnGridPane" hgap="5.0" vgap="5.0">
					<columnConstraints>
						<ColumnConstraints fillWidth="false" hgrow="NEVER" maxWidth="4150.0" minWidth="115.0" prefWidth="350.0" />
						<ColumnConstraints fillWidth="false" hgrow="NEVER" maxWidth="77.0" minWidth="77.0" prefWidth="77.0" />
						<ColumnConstraints hgrow="SOMETIMES" minWidth="100.0" prefWidth="100.0" />
					</columnConstraints>
					<children>
						<Label text="No Rows" GridPane.columnSpan="3" GridPane.halignment="CENTER" />
					</children>
					<rowConstraints>
						<RowConstraints />
					</rowConstraints>
				</GridPane>
			</content>
		</ScrollPane>
	</center>
</BorderPane>
