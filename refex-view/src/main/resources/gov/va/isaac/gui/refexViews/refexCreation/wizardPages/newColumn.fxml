						<Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
					</VBox.margin>
				</TextArea>
				<GridPane>
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" prefWidth="100.0" />
						<ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" prefWidth="100.0" />
					</columnConstraints>
					<rowConstraints>
						<RowConstraints vgrow="SOMETIMES" />
					</rowConstraints>
					<children>
						<Button fx:id="commitButton" mnemonicParsing="false" text="Create" GridPane.columnIndex="1">
							<VBox.margin>
								<Insets top="10.0" />
							</VBox.margin>
							<GridPane.margin>
								<Insets bottom="15.0" left="15.0" right="5.0" top="5.0" />
							</GridPane.margin>
						</Button>
						<Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" GridPane.halignment="RIGHT">
							<GridPane.margin>
								<Insets bottom="15.0" left="5.0" right="15.0" top="5.0" />
							</GridPane.margin>
						</Button>
					</children>
				</GridPane>
			</children>
			<opaqueInsets>
				<Insets />
			</opaqueInsets>
		</VBox>
	</children>
