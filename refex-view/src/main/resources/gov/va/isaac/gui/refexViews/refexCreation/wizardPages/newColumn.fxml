<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:id="rootPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	styleClass="dialogBackground" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gov.va.isaac.gui.refexViews.refexCreation.wizardPages.NewColumnDialogController">
	<children>
		<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
			minWidth="-Infinity" prefWidth="460.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<Label alignment="TOP_LEFT" text="Enter Column Name &amp; Description">
					<font>
						<Font name="System Bold" size="18.0" />
					</font>
					<VBox.margin>
						<Insets bottom="15.0" left="40.0" right="40.0" top="15.0" />
					</VBox.margin>
				</Label>
				<TextArea fx:id="newColName" maxWidth="1.7976931348623157E308" prefHeight="36.0" promptText="Enter Column Name"
					wrapText="true">
					<VBox.margin>
						<Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
					</VBox.margin>
				</TextArea>
				<TextArea fx:id="newColDesc" maxWidth="1.7976931348623157E308" prefHeight="70.0" promptText="Enter Column Description"
					wrapText="true">
