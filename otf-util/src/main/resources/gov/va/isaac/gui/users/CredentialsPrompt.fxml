<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="198.0" prefWidth="500.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.va.isaac.gui.users.CredentialsPromptDialogController">
	<top>
		<VBox alignment="CENTER">
			<children>
				<Label text="Credentials Request" BorderPane.alignment="CENTER">
					<font>
						<Font name="System Bold" size="20.0" />
					</font>
				</Label>
				<Label fx:id="detailsLabel" text="specify details here...">
					<VBox.margin>
						<Insets bottom="10.0" top="10.0" />
					</VBox.margin>
				</Label>
			</children>
		</VBox>
	</top>
	<center>
		<GridPane fx:id="layoutPane" hgap="10.0" vgap="5.0" BorderPane.alignment="CENTER">
			<columnConstraints>
				<ColumnConstraints hgrow="NEVER" />
				<ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="30.0" vgrow="NEVER" />
				<RowConstraints minHeight="30.0" vgrow="NEVER" />
			</rowConstraints>
			<children>
				<Label text="Username">
					<font>
						<Font name="System Bold" size="13.0" />
					</font>
				</Label>
				<Label text="Password" GridPane.rowIndex="1">
					<font>
						<Font name="System Bold" size="13.0" />
					</font>
				</Label>
				<TextField fx:id="username" GridPane.columnIndex="1" />
				<PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
			</children>
		</GridPane>
	</center>
	<bottom>
		<HBox alignment="CENTER" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="okButton" mnemonicParsing="false" text="  Ok  ">
					<HBox.margin>
						<Insets right="20.0" />
					</HBox.margin>
				</Button>
				<Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel">
					<HBox.margin>
						<Insets left="20.0" />
					</HBox.margin>
				</Button>
			</children>
			<padding>
				<Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
			</padding>
		</HBox>
	</bottom>
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
</BorderPane>
