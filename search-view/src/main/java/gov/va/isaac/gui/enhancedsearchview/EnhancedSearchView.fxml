<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="pane" prefWidth="700.0" styleClass="defaultBackgroundColor" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.va.isaac.gui.enhancedsearchview.EnhancedSearchViewController">
  <stylesheets>
    <URL value="@EnhancedSearchView.css" />
  </stylesheets>
  <top>
    <HBox>
      <children>
            <HBox HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="addFilterButton" maxWidth="50.0" mnemonicParsing="false" text="Add Filter" textAlignment="CENTER" wrapText="true" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="clearFiltersButton" mnemonicParsing="false" prefWidth="50.0" text="Clear Filters" textAlignment="CENTER" wrapText="true" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <VBox fx:id="searchAndFilterVBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <children>
                        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                           <children>
                              <Label alignment="CENTER" maxWidth="-Infinity" prefWidth="50.0" text="Search Strategy" textAlignment="CENTER" wrapText="true" />
                              <ComboBox fx:id="searchStrategyComboBox" maxWidth="1.7976931348623157E308" onAction="#load" HBox.hgrow="ALWAYS">
                                 <HBox.margin>
                                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                 </HBox.margin>
                              </ComboBox>
                           </children>
                        </HBox>
                        <VBox fx:id="dynamicFilterVBox" maxWidth="1.7976931348623157E308" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
               <children>
                  <Label prefWidth="50.0" text="Search Param" textAlignment="CENTER" wrapText="true" HBox.hgrow="NEVER" />
              <TextField fx:id="searchText" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </HBox.margin>
              </TextField>
               </children>
            </HBox>
            <ToolBar HBox.hgrow="ALWAYS">
               <items>
              <Button fx:id="searchButton" mnemonicParsing="false" text="Run" textAlignment="CENTER" />
               </items>
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </ToolBar>
      </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
    </HBox>
  </top>
   <center>
      <TableView fx:id="searchResultsTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
