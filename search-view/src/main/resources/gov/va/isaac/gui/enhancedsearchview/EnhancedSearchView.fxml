<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="pane" prefWidth="700.0" styleClass="defaultBackgroundColor" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.va.isaac.gui.enhancedsearchview.EnhancedSearchViewController">
  <stylesheets>
    <URL value="@EnhancedSearchView.css" />
  </stylesheets>
   <center>
      <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <TableView fx:id="searchResultsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER" />
            <ProgressIndicator fx:id="searchProgress" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="100.0" prefWidth="100.0" />
         </children>
      </StackPane>
   </center>
   <top>
      <FlowPane maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <children>
            <ComboBox fx:id="aggregationTypeComboBox" promptText="Search Type">
               <FlowPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </FlowPane.margin>
            </ComboBox>
            <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
               <children>
                  <Label text="Search Param" textAlignment="CENTER" wrapText="true" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin></Label>
              <TextField fx:id="searchText" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                </HBox.margin>
              </TextField>
              <Button fx:id="searchButton" mnemonicParsing="false" text="Search" textAlignment="CENTER" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="2.0" right="5.0" top="5.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <Label text="Export" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="2.0" top="5.0" />
                     </HBox.margin></Label>
                  <Button fx:id="exportSearchResultsAsTabDelimitedValuesButton" mnemonicParsing="false" text="File" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="2.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="exportSearchResultsToListBatchViewButton" mnemonicParsing="false" text="List" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="2.0" right="2.0" top="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="exportSearchResultsToWorkflowButton" mnemonicParsing="false" text="Workflow" HBox.hgrow="NEVER">
                     <HBox.margin>
                        <Insets bottom="5.0" left="2.0" right="5.0" top="5.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
      </FlowPane>
   </top>
   <bottom>
      <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="totalResultsDisplayedLabel" text="Total" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </bottom>
</BorderPane>
