<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="borderPane" prefWidth="300.0" styleClass="defaultBackgroundColor" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.va.isaac.gui.searchview.EnhancedSearchViewController">
  <stylesheets>
    <URL value="@SimpleSearchView.css" />
  </stylesheets>
  <top>
    <HBox>
      <children>
        <TextField fx:id="searchText" HBox.hgrow="ALWAYS">
          <HBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </HBox.margin>
        </TextField>
        <Button fx:id="searchButton" maxWidth="-Infinity" mnemonicParsing="false" prefWidth="75.0" text="Search" HBox.hgrow="NEVER">
          <HBox.margin>
            <Insets bottom="5.0" right="5.0" top="5.0" />
          </HBox.margin>
        </Button>
      </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
    </HBox>
  </top>
</BorderPane>
