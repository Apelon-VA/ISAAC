<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane fx:id="conceptCreationPane" prefHeight="419.0" prefWidth="591.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gov.va.isaac.gui.conceptCreation.wizardPages.DefinitionController">
	<center>
		<GridPane fx:id="gridPane" hgap="10.0" vgap="10.0">
			<columnConstraints>
				<ColumnConstraints fillWidth="false" halignment="RIGHT" hgrow="NEVER" maxWidth="187.0" minWidth="61.0" prefWidth="74.0" />
				<ColumnConstraints halignment="LEFT" hgrow="NEVER" maxWidth="209.0" minWidth="10.0" prefWidth="74.0" />
				<ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="344.0" minWidth="63.0" prefWidth="337.0" />
				<ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
				<ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="25.0" minWidth="25.0" prefWidth="25.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints vgrow="NEVER" />
				<RowConstraints vgrow="NEVER" />
				<RowConstraints vgrow="NEVER" />
				<RowConstraints />
			</rowConstraints>
			<children>
				<Label text="FSN" />
				<Label text="Pref Term" GridPane.rowIndex="1" />
				<TextField fx:id="fsn" maxHeight="25.0" minHeight="25.0" prefHeight="25.0" prefWidth="338.0" GridPane.columnIndex="1"
					GridPane.columnSpan="4" />
				<TextArea fx:id="prefTerm" maxHeight="25.0" minHeight="25.0" prefHeight="25.0" prefWidth="396.0" wrapText="true"
					GridPane.columnIndex="1" GridPane.columnSpan="4" GridPane.rowIndex="1" />
				<Label text="Parents" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
				<VBox fx:id="parentVBox" prefWidth="100.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
				<VBox fx:id="addParentButtonVBox" alignment="CENTER" prefWidth="100.0" GridPane.columnIndex="3" GridPane.rowIndex="2" />
				<VBox fx:id="removeParentButtonVBox" alignment="CENTER" prefWidth="100.0" GridPane.columnIndex="4" GridPane.rowIndex="2" />
				<HBox GridPane.columnIndex="2" GridPane.rowIndex="3">
					<children>
						<RadioButton fx:id="isPrimitive" mnemonicParsing="false" selected="true" text="Primitive" GridPane.columnIndex="1"
							GridPane.rowIndex="4">
							<HBox.margin>
								<Insets left="5.0" right="5.0" />
							</HBox.margin>
							<padding>
								<Insets left="5.0" right="5.0" />
							</padding>
							<toggleGroup>
								<ToggleGroup fx:id="conceptTypeGroup" />
							</toggleGroup>
						</RadioButton>
						<RadioButton fx:id="isFullyDefined" mnemonicParsing="false" text="Fully Defined" toggleGroup="$conceptTypeGroup"
							GridPane.rowIndex="4">
							<HBox.margin>
								<Insets left="5.0" right="5.0" />
							</HBox.margin>
							<padding>
								<Insets left="5.0" right="5.0" />
							</padding>
						</RadioButton>
					</children>
				</HBox>
			</children>
		</GridPane>
	</center>
	<top>
		<Label text="New Concept Definition" BorderPane.alignment="CENTER">
			<font>
				<Font name="System Bold" size="24.0" />
			</font>
			<BorderPane.margin>
				<Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
			</BorderPane.margin>
		</Label>
	</top>
	<bottom>
		<GridPane>
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Button fx:id="cancelCreation" mnemonicParsing="false" text="Cancel" GridPane.halignment="RIGHT" GridPane.hgrow="SOMETIMES"
					HBox.hgrow="SOMETIMES">
					<HBox.margin>
						<Insets bottom="5.0" left="5.0" right="15.0" top="5.0" />
					</HBox.margin>
					<GridPane.margin>
						<Insets bottom="5.0" left="5.0" right="15.0" top="5.0" />
					</GridPane.margin>
				</Button>
				<Button fx:id="continueCreation" mnemonicParsing="false" text="Next" GridPane.columnIndex="1" GridPane.hgrow="SOMETIMES"
					HBox.hgrow="SOMETIMES">
					<GridPane.margin>
						<Insets bottom="5.0" left="15.0" right="5.0" top="5.0" />
					</GridPane.margin>
				</Button>
			</children>
		</GridPane>
	</bottom>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</BorderPane>
